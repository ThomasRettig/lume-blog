{% set prevPost = search.previousPage(url, "post", "date=desc") %}
{% set nextPost = search.nextPage(url, "post", "date=desc") %}
<nav class="post-navigation">
    {% if prevPost %}
    <div>
        <h2>Newer</h2>
        <a href="{{ prevPost.data.url }}" rel="prev">{{ prevPost.data.title }}</a>
    </div>
    {% endif %}

    {% if nextPost %}
    <div>
        <h2>Older</h2>
        <a href="{{ nextPost.data.url }}" rel="next">{{ nextPost.data.title }}</a>
    </div>
    {% endif %}
</nav>
